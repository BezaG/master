  <h1>Welcome to Addis Farm Market</h1>
  <% if user_signed_in? %>
      <p>Current user: <%= current_user.email %></p>
      <li><%= link_to('Update Profile', edit_user_registration_path, :class => 'btn btn-default') %></li>
      <li><%= link_to('Logout', destroy_user_session_path, :method => :delete, :class => 'btn btn-default') %> </li>
  <% else %>
      <%= link_to 'Sign in', new_user_session_path, :class => 'btn btn-default' %>
      <%= link_to 'Sign Up', new_user_registration_path, :class => 'btn btn-success' %>
  <% end %>
